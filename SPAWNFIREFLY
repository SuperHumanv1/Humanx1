-- Firefly Spawner UI for Delta Executor
local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- à¸ªà¸£à¹‰à¸²à¸‡ GUI
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "FireflySpawner"
screenGui.Parent = player:WaitForChild("PlayerGui")

local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 250, 0, 180)
frame.Position = UDim2.new(0.5, -125, 0.5, -90)
frame.BackgroundColor3 = Color3.fromRGB(30, 30, 35)
frame.BorderSizePixel = 0
frame.Parent = screenGui

-- à¸«à¸™à¹‰à¸²à¸•à¹ˆà¸²à¸‡à¸ˆà¸±à¸šà¸¢à¹‰à¸²à¸¢ (draggable area)
local dragFrame = Instance.new("Frame")
dragFrame.Size = UDim2.new(1, 0, 0, 40)
dragFrame.BackgroundTransparency = 1
dragFrame.Active = true  -- à¸—à¸³à¹ƒà¸«à¹‰à¸ˆà¸±à¸šà¹„à¸”à¹‰
dragFrame.Selectable = false
dragFrame.Parent = frame

local title = Instance.new("TextLabel")
title.Text = "ğŸ”¥ Firefly Spawner (à¸„à¸¥à¸´à¸à¸„à¹‰à¸²à¸‡à¸¥à¸²à¸à¸¢à¹‰à¸²à¸¢)"
title.Size = UDim2.new(1, 0, 0, 40)
title.BackgroundColor3 = Color3.fromRGB(25, 25, 30)
title.TextColor3 = Color3.fromRGB(255, 200, 0)
title.TextSize = 18
title.Font = Enum.Font.GothamBold
title.Parent = dragFrame

local inputBox = Instance.new("TextBox")
inputBox.PlaceholderText = "à¸ˆà¸³à¸™à¸§à¸™à¸—à¸µà¹ˆà¸ˆà¸°à¹€à¸ªà¸ (à¹€à¸Šà¹ˆà¸™: 10)"
inputBox.Size = UDim2.new(0.8, 0, 0, 35)
inputBox.Position = UDim2.new(0.1, 0, 0.3, 0)
inputBox.BackgroundColor3 = Color3.fromRGB(40, 40, 45)
inputBox.TextColor3 = Color3.fromRGB(255, 255, 255)
inputBox.Text = "10"
inputBox.Parent = frame

local spawnButton = Instance.new("TextButton")
spawnButton.Text = "ğŸš€ à¹€à¸ªà¸à¹€à¸”à¸µà¹‹à¸¢à¸§à¸™à¸µà¹‰!"
spawnButton.Size = UDim2.new(0.8, 0, 0, 40)
spawnButton.Position = UDim2.new(0.1, 0, 0.55, 0)
spawnButton.BackgroundColor3 = Color3.fromRGB(0, 150, 80)
spawnButton.TextColor3 = Color3.fromRGB(255, 255, 255)
spawnButton.TextSize = 16
spawnButton.Font = Enum.Font.GothamSemibold
spawnButton.Parent = frame

local statusLabel = Instance.new("TextLabel")
statusLabel.Text = "à¸à¸£à¹‰à¸­à¸¡à¹ƒà¸Šà¹‰à¸‡à¸²à¸™"
statusLabel.Size = UDim2.new(1, 0, 0, 25)
statusLabel.Position = UDim2.new(0, 0, 0.85, 0)
statusLabel.BackgroundTransparency = 1
statusLabel.TextColor3 = Color3.fromRGB(150, 255, 150)
statusLabel.TextSize = 14
statusLabel.Parent = frame

-- à¸›à¸¸à¹ˆà¸¡à¸›à¸´à¸” UI
local closeButton = Instance.new("TextButton")
closeButton.Text = "âœ•"
closeButton.Size = UDim2.new(0, 30, 0, 30)
closeButton.Position = UDim2.new(1, -30, 0, 5)
closeButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
closeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
closeButton.TextSize = 16
closeButton.Font = Enum.Font.GothamBold
closeButton.Parent = dragFrame

-- à¸Ÿà¸±à¸‡à¸à¹Œà¸Šà¸±à¸™à¸¥à¸²à¸à¸¢à¹‰à¸²à¸¢ UI
local UserInputService = game:GetService("UserInputService")
local dragging
local dragInput
local dragStart
local startPos

local function updateInput(input)
    local delta = input.Position - dragStart
    frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

dragFrame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = frame.Position
        
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

dragFrame.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        dragInput = input
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if dragging and (input == dragInput or input.UserInputType == Enum.UserInputType.MouseMovement) then
        updateInput(input)
    end
end)

-- à¸›à¸´à¸” UI
closeButton.MouseButton1Click:Connect(function()
    screenGui:Destroy()
end)

-- à¸Ÿà¸±à¸‡à¸à¹Œà¸Šà¸±à¸™à¹€à¸ªà¸ Fireflies
local function spawnFireflies(count)
    if not player.Backpack:FindFirstChild("Fireflies") then
        statusLabel.Text = "âŒ à¹„à¸¡à¹ˆà¸à¸š Fireflies"
        statusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        return
    end
    
    local remoteEvent = player.Backpack.Fireflies.RemoteEvent
    statusLabel.Text = "à¸à¸³à¸¥à¸±à¸‡à¹€à¸ªà¸ " .. count .. " à¸„à¸£à¸±à¹‰à¸‡..."
    statusLabel.TextColor3 = Color3.fromRGB(255, 255, 150)
    
    for i = 1, count do
        remoteEvent:FireServer()
        wait(0.08)  -- à¸„à¸§à¸²à¸¡à¹€à¸£à¹‡à¸§à¹ƒà¸™à¸à¸²à¸£à¹€à¸ªà¸
    end
    
    statusLabel.Text = "âœ… à¹€à¸ªà¸à¸ªà¸³à¹€à¸£à¹‡à¸ˆ " .. count .. " à¸„à¸£à¸±à¹‰à¸‡!"
    statusLabel.TextColor3 = Color3.fromRGB(150, 255, 150)
    
    -- à¸£à¸µà¹€à¸‹à¹‡à¸•à¸‚à¹‰à¸­à¸„à¸§à¸²à¸¡à¸«à¸¥à¸±à¸‡à¸ˆà¸²à¸ 3 à¸§à¸´à¸™à¸²à¸—à¸µ
    wait(3)
    statusLabel.Text = "à¸à¸£à¹‰à¸­à¸¡à¹ƒà¸Šà¹‰à¸‡à¸²à¸™"
    statusLabel.TextColor3 = Color3.fromRGB(150, 255, 150)
end

-- à¸›à¸¸à¹ˆà¸¡à¸à¸”à¹€à¸ªà¸
spawnButton.MouseButton1Click:Connect(function()
    local count = tonumber(inputBox.Text)
    if count and count > 0 and count <= 100 then
        spawnFireflies(count)
    else
        statusLabel.Text = "âŒ à¸à¸£à¸­à¸à¸•à¸±à¸§à¹€à¸¥à¸‚ 1-100"
        statusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
    end
end)

-- à¸£à¸°à¸šà¸šà¸„à¸³à¸ªà¸±à¹ˆà¸‡à¹à¸Šà¸— "!firefly"
game:GetService("Players").LocalPlayer.Chatted:Connect(function(message)
    if string.lower(message) == "!firefly" then
        spawnFireflies(10)  -- à¹€à¸ªà¸ 10 à¸„à¸£à¸±à¹‰à¸‡à¹‚à¸”à¸¢à¸­à¸±à¸•à¹‚à¸™à¸¡à¸±à¸•à¸´
    elseif string.lower(string.sub(message, 1, 9)) == "!firefly " then
        local count = tonumber(string.sub(message, 10))
        if count and count > 0 and count <= 100 then
            spawnFireflies(count)
        end
    end
end)

print("âœ… Firefly Spawner à¸à¸£à¹‰à¸­à¸¡à¹ƒà¸Šà¹‰à¸‡à¸²à¸™à¹à¸¥à¹‰à¸§!")
print("à¹ƒà¸Šà¹‰à¸„à¸³à¸ªà¸±à¹ˆà¸‡à¹ƒà¸™à¹à¸Šà¸—: !firefly à¸«à¸£à¸·à¸­ !firefly [à¸ˆà¸³à¸™à¸§à¸™]")
print("à¸«à¸£à¸·à¸­à¹ƒà¸Šà¹‰ UI à¸—à¸µà¹ˆà¸›à¸£à¸²à¸à¸à¸šà¸™à¸«à¸™à¹‰à¸²à¸ˆà¸­")
print("à¸„à¸¥à¸´à¸à¸—à¸µà¹ˆà¹à¸–à¸šà¸šà¸™à¸ªà¸¸à¸”à¹à¸¥à¹‰à¸§à¸¥à¸²à¸à¹€à¸à¸·à¹ˆà¸­à¸¢à¹‰à¸²à¸¢ UI | à¸à¸” âœ• à¹€à¸à¸·à¹ˆà¸­à¸›à¸´à¸”")

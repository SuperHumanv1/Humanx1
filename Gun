-- üîß FIXED GUN BUY SCRIPT
-- ‚úÖ ‡πÅ‡∏Å‡πâ‡πÑ‡∏Ç‡πÅ‡∏•‡πâ‡∏ß‡∏ã‡∏∑‡πâ‡∏≠‡∏õ‡∏∑‡∏ô‡πÑ‡∏î‡πâ‡∏à‡∏£‡∏¥‡∏á ‡∏à‡∏≤‡∏Å ClickDetector

local Workspace = game:GetService("Workspace")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£
local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

print("üîß FIXED GUN BUY SCRIPT LOADED!")
print("=" .. string.rep("=", 40))

-- ============================================
-- üîç ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡∏£‡πâ‡∏≤‡∏ô‡∏Ñ‡πâ‡∏≤‡∏ó‡∏µ‡πà‡∏ñ‡∏π‡∏Å‡∏ï‡πâ‡∏≠‡∏á
-- ============================================

local function findGunShop()
    print("üîç ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡∏£‡πâ‡∏≤‡∏ô‡∏Ñ‡πâ‡∏≤‡∏õ‡∏∑‡∏ô...")
    
    local allChildren = Workspace:GetChildren()
    print("üìä ‡∏û‡∏ö objects ‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î: " .. #allChildren)
    
    -- ‡∏ß‡∏¥‡∏ò‡∏µ‡∏ó‡∏µ‡πà 1: ‡∏•‡∏≠‡∏á‡∏´‡∏•‡∏≤‡∏¢ index ‡∏£‡∏≠‡∏ö‡πÜ 418
    local possibleIndices = {418, 419, 417, 420, 416, 421}
    
    for _, indexNum in ipairs(possibleIndices) do
        local index = indexNum + 1  -- ‡πÄ‡∏û‡∏£‡∏≤‡∏∞ Lua ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ó‡∏µ‡πà 1
        
        if allChildren[index] then
            local obj = allChildren[index]
            print("\nüîé ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö index " .. indexNum .. " (" .. index .. "): " .. obj.Name .. " (" .. obj.ClassName .. ")")
            
            if obj:IsA("Model") then
                -- ‡∏´‡∏≤ ClickDetector ‡πÉ‡∏ô Model
                local clickDetector = obj:FindFirstChildWhichIsA("ClickDetector")
                
                if clickDetector then
                    print("‚úÖ ‡∏û‡∏ö ClickDetector ‡πÉ‡∏ô Model!")
                    return clickDetector, obj.Name
                end
                
                -- ‡∏´‡∏≤ Head ‡πÅ‡∏•‡πâ‡∏ß‡∏´‡∏≤‡∏î‡πâ‡∏≤‡∏ô‡πÉ‡∏ô
                local head = obj:FindFirstChild("Head")
                if head then
                    local headDetector = head:FindFirstChildWhichIsA("ClickDetector")
                    if headDetector then
                        print("‚úÖ ‡∏û‡∏ö ClickDetector ‡πÉ‡∏ô Head!")
                        return headDetector, obj.Name
                    end
                end
            end
        end
    end
    
    -- ‡∏ß‡∏¥‡∏ò‡∏µ‡∏ó‡∏µ‡πà 2: ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡πÉ‡∏ô Workspace
    print("\nüîç ‡∏Ñ‡πâ‡∏ô‡∏´‡∏≤‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡πÉ‡∏ô Workspace...")
    local allDetectors = {}
    
    for _, obj in pairs(Workspace:GetDescendants()) do
        if obj:IsA("ClickDetector") then
            table.insert(allDetectors, obj)
        end
    end
    
    print("üéØ ‡∏û‡∏ö ClickDetector ‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î: " .. #allDetectors .. " ‡∏≠‡∏±‡∏ô")
    
    if #allDetectors > 0 then
        -- ‡∏•‡∏≠‡∏á‡∏´‡∏≤ detector ‡∏ó‡∏µ‡πà‡∏°‡∏µ‡∏ä‡∏∑‡πà‡∏≠‡πÄ‡∏Å‡∏µ‡πà‡∏¢‡∏ß‡∏Å‡∏±‡∏ö‡∏£‡πâ‡∏≤‡∏ô‡∏Ñ‡πâ‡∏≤
        for _, detector in ipairs(allDetectors) do
            local parentName = detector.Parent.Name:lower()
            local fullPath = detector:GetFullName():lower()
            
            if parentName:find("shop") or parentName:find("store") or 
               parentName:find("gun") or parentName:find("weapon") or
               fullPath:find("shop") or fullPath:find("store") then
                print("‚úÖ ‡∏û‡∏ö ClickDetector ‡∏ó‡∏µ‡πà‡∏ô‡πà‡∏≤‡∏à‡∏∞‡πÄ‡∏õ‡πá‡∏ô‡∏£‡πâ‡∏≤‡∏ô‡∏Ñ‡πâ‡∏≤: " .. detector:GetFullName())
                return detector, detector.Parent.Name
            end
        end
        
        -- ‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡πÄ‡∏à‡∏≠‡πÅ‡∏ö‡∏ö‡∏û‡∏¥‡πÄ‡∏®‡∏© ‡πÉ‡∏´‡πâ‡πÉ‡∏ä‡πâ‡∏≠‡∏±‡∏ô‡πÅ‡∏£‡∏Å
        print("‚úÖ ‡πÉ‡∏ä‡πâ ClickDetector ‡πÅ‡∏£‡∏Å‡∏ó‡∏µ‡πà‡∏û‡∏ö: " .. allDetectors[1]:GetFullName())
        return allDetectors[1], allDetectors[1].Parent.Name
    end
    
    return nil, "‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏£‡πâ‡∏≤‡∏ô‡∏Ñ‡πâ‡∏≤"
end

-- ============================================
-- üõí ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ã‡∏∑‡πâ‡∏≠‡∏õ‡∏∑‡∏ô (‡πÅ‡∏Å‡πâ‡πÑ‡∏Ç‡πÉ‡∏´‡∏°‡πà)
-- ============================================

local function buyGunFixed()
    print("\nüõí ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Å‡∏£‡∏∞‡∏ö‡∏ß‡∏ô‡∏Å‡∏≤‡∏£‡∏ã‡∏∑‡πâ‡∏≠‡∏õ‡∏∑‡∏ô (‡πÄ‡∏ß‡∏≠‡∏£‡πå‡∏ä‡∏±‡∏ô‡πÅ‡∏Å‡πâ‡πÑ‡∏Ç)...")
    print("-" .. string.rep("-", 40))
    
    -- ‡∏´‡∏≤ ClickDetector
    local clickDetector, shopName = findGunShop()
    
    if not clickDetector then
        print("‚ùå ‡πÑ‡∏°‡πà‡∏û‡∏ö ClickDetector ‡∏Ç‡∏≠‡∏á‡∏£‡πâ‡∏≤‡∏ô‡∏Ñ‡πâ‡∏≤")
        return false
    end
    
    print("üéØ ‡∏û‡∏ö‡∏£‡πâ‡∏≤‡∏ô‡∏Ñ‡πâ‡∏≤: " .. shopName)
    
    -- ‡∏•‡∏≠‡∏á‡∏´‡∏•‡∏≤‡∏¢‡∏ß‡∏¥‡∏ò‡∏µ‡πÉ‡∏ô‡∏Å‡∏≤‡∏£‡∏Ñ‡∏•‡∏¥‡∏Å
    local clickedSuccess = false
    
    for attempt = 1, 3 do
        print("üîÑ ‡∏•‡∏≠‡∏á‡∏Ñ‡∏•‡∏¥‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏ó‡∏µ‡πà " .. attempt)
        
        -- ‡∏ß‡∏¥‡∏ò‡∏µ‡∏ó‡∏µ‡πà 1: fireclickdetector
        fireclickdetector(clickDetector)
        
        -- ‡∏ß‡∏¥‡∏ò‡∏µ‡∏ó‡∏µ‡πà 2: ‡∏•‡∏≠‡∏á‡∏Ñ‡∏•‡∏¥‡∏Å‡πÅ‡∏ö‡∏ö‡∏≠‡∏∑‡πà‡∏ô‡∏ñ‡πâ‡∏≤‡πÄ‡∏õ‡πá‡∏ô ClickDetector
        if clickDetector:IsA("ClickDetector") then
            pcall(function()
                clickDetector:MouseClick()
            end)
            
            pcall(function()
                clickDetector.MouseButton1Click:Fire()
            end)
        end
        
        -- ‡∏£‡∏≠‡∏™‡∏±‡∏Å‡∏Ñ‡∏£‡∏π‡πà
        task.wait(1)
        
        -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡∏ã‡∏∑‡πâ‡∏≠‡πÑ‡∏î‡πâ‡πÑ‡∏´‡∏°
        local backpack = player:FindFirstChild("Backpack")
        if backpack then
            local tools = {}
            for _, item in pairs(backpack:GetChildren()) do
                if item:IsA("Tool") then
                    table.insert(tools, item)
                end
            end
            
            local character = player.Character
            if character then
                for _, item in pairs(character:GetChildren()) do
                    if item:IsA("Tool") then
                        table.insert(tools, item)
                    end
                end
            end
            
            if #tools > 0 then
                print("‚úÖ ‡∏ã‡∏∑‡πâ‡∏≠‡∏õ‡∏∑‡∏ô‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à! ‡πÑ‡∏î‡πâ " .. #tools .. " ‡∏≠‡∏±‡∏ô")
                for i, tool in ipairs(tools) do
                    print("   " .. i .. ". " .. tool.Name)
                end
                clickedSuccess = true
                break
            end
        end
        
        -- ‡∏ñ‡πâ‡∏≤‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à ‡∏£‡∏≠‡πÅ‡∏•‡πâ‡∏ß‡∏•‡∏≠‡∏á‡πÉ‡∏´‡∏°‡πà
        task.wait(1)
    end
    
    if not clickedSuccess then
        print("‚ö†Ô∏è ‡∏Ñ‡∏•‡∏¥‡∏Å‡πÅ‡∏•‡πâ‡∏ß‡πÅ‡∏ï‡πà‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏õ‡∏∑‡∏ô‡∏ó‡∏µ‡πà‡∏ã‡∏∑‡πâ‡∏≠‡πÑ‡∏î‡πâ")
        
        -- ‡∏•‡∏≠‡∏á‡∏Ñ‡∏•‡∏¥‡∏Å‡∏ó‡∏∏‡∏Å ClickDetector ‡πÉ‡∏ô‡πÄ‡∏Å‡∏° (‡∏ß‡∏¥‡∏ò‡∏µ‡∏™‡∏∏‡∏î‡∏ó‡πâ‡∏≤‡∏¢)
        print("\nüîÑ ‡∏•‡∏≠‡∏á‡∏Ñ‡∏•‡∏¥‡∏Å‡∏ó‡∏∏‡∏Å ClickDetector ‡πÉ‡∏ô‡πÄ‡∏Å‡∏°...")
        local allDetectors = {}
        
        for _, obj in pairs(Workspace:GetDescendants()) do
            if obj:IsA("ClickDetector") then
                table.insert(allDetectors, obj)
            end
        end
        
        print("üéØ ‡∏à‡∏∞‡∏Ñ‡∏•‡∏¥‡∏Å‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î " .. #allDetectors .. " ‡∏≠‡∏±‡∏ô")
        
        for i, detector in ipairs(allDetectors) do
            print("   ‡∏Ñ‡∏•‡∏¥‡∏Å " .. i .. ": " .. detector:GetFullName())
            fireclickdetector(detector)
            task.wait(0.5)
            
            -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡∏ã‡∏∑‡πâ‡∏≠‡πÑ‡∏î‡πâ‡πÑ‡∏´‡∏°
            local backpack = player:FindFirstChild("Backpack")
            if backpack then
                local hasTool = false
                for _, item in pairs(backpack:GetChildren()) do
                    if item:IsA("Tool") then
                        hasTool = true
                        break
                    end
                end
                
                if hasTool then
                    print("‚úÖ ‡∏Ñ‡∏•‡∏¥‡∏Å‡∏≠‡∏±‡∏ô‡∏ó‡∏µ‡πà " .. i .. " ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!")
                    clickedSuccess = true
                    break
                end
            end
        end
    end
    
    return clickedSuccess
end

-- ============================================
-- üîß ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏î‡∏™‡∏≠‡∏ö ClickDetector ‡πÇ‡∏î‡∏¢‡∏ï‡∏£‡∏á
-- ============================================

local function testDirectClick()
    print("\nüîß ‡∏ó‡∏î‡∏™‡∏≠‡∏ö‡∏Ñ‡∏•‡∏¥‡∏Å‡∏ï‡∏£‡∏á index 418...")
    
    local success, result = pcall(function()
        -- ‡∏•‡∏≠‡∏á‡πÉ‡∏ä‡πâ‡πÇ‡∏Ñ‡πâ‡∏î‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì‡∏ï‡∏£‡∏á‡πÜ
        local children = Workspace:GetChildren()
        
        if #children > 418 then
            local obj = children[419]  -- 418 + 1
            
            if obj then
                print("‚úÖ ‡∏û‡∏ö object: " .. obj.Name .. " (" .. obj.ClassName .. ")")
                
                -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡πÄ‡∏õ‡πá‡∏ô Model ‡πÅ‡∏•‡∏∞‡∏°‡∏µ Head
                if obj:IsA("Model") and obj:FindFirstChild("Head") then
                    local head = obj.Head
                    
                    if head:FindFirstChild("ClickDetector") then
                        local detector = head.ClickDetector
                        print("üéØ ‡∏û‡∏ö ClickDetector! ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏Ñ‡∏•‡∏¥‡∏Å...")
                        
                        -- ‡∏Ñ‡∏•‡∏¥‡∏Å‡∏´‡∏•‡∏≤‡∏¢‡∏Ñ‡∏£‡∏±‡πâ‡∏á
                        for i = 1, 5 do
                            fireclickdetector(detector)
                            print("   ‡∏Ñ‡∏•‡∏¥‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏ó‡∏µ‡πà " .. i)
                            task.wait(0.3)
                        end
                        
                        return true
                    else
                        print("‚ùå ‡πÑ‡∏°‡πà‡∏°‡∏µ ClickDetector ‡πÉ‡∏ô Head")
                    end
                else
                    print("‚ùå ‡πÑ‡∏°‡πà‡πÉ‡∏ä‡πà Model ‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà‡∏°‡∏µ Head")
                end
            end
        end
        
        return false
    end)
    
    if not success then
        print("‚ùå ‡πÄ‡∏Å‡∏¥‡∏î‡∏Ç‡πâ‡∏≠‡∏ú‡∏¥‡∏î‡∏û‡∏•‡∏≤‡∏î: " .. tostring(result))
    end
    
    return success
end

-- ============================================
-- üéÆ ‡∏£‡∏∞‡∏ö‡∏ö‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥
-- ============================================

-- ‡∏ã‡∏∑‡πâ‡∏≠‡∏õ‡∏∑‡∏ô‡∏ó‡∏±‡∏ô‡∏ó‡∏µ‡∏ó‡∏µ‡πà‡πÇ‡∏´‡∏•‡∏î‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå
task.spawn(function()
    print("\n‚ö° ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ã‡∏∑‡πâ‡∏≠‡∏õ‡∏∑‡∏ô‡∏≠‡∏±‡∏ï‡πÇ‡∏ô‡∏°‡∏±‡∏ï‡∏¥...")
    
    -- ‡∏ß‡∏¥‡∏ò‡∏µ‡∏ó‡∏µ‡πà 1: ‡πÉ‡∏ä‡πâ‡∏ß‡∏¥‡∏ò‡∏µ‡πÅ‡∏Å‡πâ‡πÑ‡∏Ç‡πÉ‡∏´‡∏°‡πà
    local bought = buyGunFixed()
    
    if not bought then
        -- ‡∏ß‡∏¥‡∏ò‡∏µ‡∏ó‡∏µ‡πà 2: ‡∏•‡∏≠‡∏á‡∏ó‡∏î‡∏™‡∏≠‡∏ö‡∏Ñ‡∏•‡∏¥‡∏Å‡∏ï‡∏£‡∏á‡πÜ
        print("\nüîÑ ‡∏•‡∏≠‡∏á‡∏ß‡∏¥‡∏ò‡∏µ‡∏ó‡∏µ‡πà 2: ‡∏ó‡∏î‡∏™‡∏≠‡∏ö‡∏Ñ‡∏•‡∏¥‡∏Å‡∏ï‡∏£‡∏á‡πÜ...")
        testDirectClick()
        
        -- ‡∏£‡∏≠‡πÅ‡∏•‡πâ‡∏ß‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏≠‡∏µ‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á
        task.wait(3)
        
        -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏≠‡∏µ‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏ß‡πà‡∏≤‡∏°‡∏µ‡∏õ‡∏∑‡∏ô‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà
        local backpack = player:FindFirstChild("Backpack")
        local hasGun = false
        
        if backpack then
            for _, item in pairs(backpack:GetChildren()) do
                if item:IsA("Tool") then
                    hasGun = true
                    break
                end
            end
        end
        
        if hasGun then
            print("üéâ ‡∏ã‡∏∑‡πâ‡∏≠‡∏õ‡∏∑‡∏ô‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à‡πÉ‡∏ô‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î!")
        else
            print("‚ùå ‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡∏ã‡∏∑‡πâ‡∏≠‡∏õ‡∏∑‡∏ô‡πÑ‡∏î‡πâ")
            print("\nüí° ‡∏•‡∏≠‡∏á‡∏ó‡∏≥‡∏ï‡∏≤‡∏°‡∏ô‡∏µ‡πâ:")
            print("1. ‡∏¢‡∏∑‡∏ô‡πÉ‡∏Å‡∏•‡πâ‡∏£‡πâ‡∏≤‡∏ô‡∏Ñ‡πâ‡∏≤‡πÉ‡∏´‡πâ‡∏°‡∏≤‡∏Å‡∏ó‡∏µ‡πà‡∏™‡∏∏‡∏î")
            print("2. ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡∏°‡∏µ‡πÄ‡∏á‡∏¥‡∏ô‡∏û‡∏≠‡∏ã‡∏∑‡πâ‡∏≠‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà")
            print("3. ‡∏•‡∏≠‡∏á‡∏ã‡∏∑‡πâ‡∏≠‡∏î‡πâ‡∏ß‡∏¢‡∏ï‡∏±‡∏ß‡πÄ‡∏≠‡∏á‡∏Å‡πà‡∏≠‡∏ô")
            print("4. ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö Console (F9) ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÄ‡∏ï‡∏¥‡∏°")
        end
    end
end)

-- ============================================
-- üí¨ ‡∏£‡∏∞‡∏ö‡∏ö‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á‡πÅ‡∏ä‡∏ó
-- ============================================

player.Chatted:Connect(function(message)
    local cmd = message:lower()
    
    if cmd == "/buy" or cmd == "/gun" then
        print("\nüîÑ ‡πÑ‡∏î‡πâ‡∏£‡∏±‡∏ö‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á‡∏ã‡∏∑‡πâ‡∏≠‡∏õ‡∏∑‡∏ô‡πÉ‡∏´‡∏°‡πà")
        buyGunFixed()
        
    elseif cmd == "/test" then
        print("\nüîß ‡∏ó‡∏î‡∏™‡∏≠‡∏ö‡∏Ñ‡∏•‡∏¥‡∏Å‡∏ï‡∏£‡∏á‡πÜ...")
        testDirectClick()
        
    elseif cmd == "/check" then
        print("\nüìä ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏õ‡∏∑‡∏ô‡πÉ‡∏ô‡∏ï‡∏±‡∏ß:")
        local backpack = player:FindFirstChild("Backpack")
        local toolCount = 0
        
        if backpack then
            for _, item in pairs(backpack:GetChildren()) do
                if item:IsA("Tool") then
                    toolCount = toolCount + 1
                    print("   Backpack: " .. item.Name)
                end
            end
        end
        
        local character = player.Character
        if character then
            for _, item in pairs(character:GetChildren()) do
                if item:IsA("Tool") then
                    toolCount = toolCount + 1
                    print("   Character: " .. item.Name)
                end
            end
        end
        
        print("üìã ‡∏û‡∏ö‡∏õ‡∏∑‡∏ô‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î: " .. toolCount .. " ‡∏≠‡∏±‡∏ô")
        
    elseif cmd == "/help" then
        print("\nüéÆ ‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î:")
        print("   /buy - ‡∏ã‡∏∑‡πâ‡∏≠‡∏õ‡∏∑‡∏ô (‡∏ß‡∏¥‡∏ò‡∏µ‡πÅ‡∏Å‡πâ‡πÑ‡∏Ç‡πÉ‡∏´‡∏°‡πà)")
        print("   /test - ‡∏ó‡∏î‡∏™‡∏≠‡∏ö‡∏Ñ‡∏•‡∏¥‡∏Å‡∏ï‡∏£‡∏á index 418")
        print("   /check - ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏õ‡∏∑‡∏ô‡πÉ‡∏ô‡∏ï‡∏±‡∏ß")
        print("   /help - ‡πÅ‡∏™‡∏î‡∏á‡∏Ñ‡∏≥‡∏™‡∏±‡πà‡∏á‡∏ô‡∏µ‡πâ")
    end
end)

-- ============================================
-- üìä ‡∏™‡∏£‡∏∏‡∏õ
-- ============================================

task.wait(5)

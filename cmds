    {name = "!lag [‡∏ä‡∏∑‡πà‡∏≠]", desc = "‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡πÅ‡∏•‡πá‡∏Å", example = "!lag P1"},
    {name = "!copy [‡∏ä‡∏∑‡πà‡∏≠]", desc = "‡∏Å‡πä‡∏≠‡∏õ‡∏õ‡∏µ‡πâ‡∏ä‡∏∏‡∏î", example = "!copy P1"},

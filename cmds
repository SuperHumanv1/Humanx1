-- ‡∏™‡∏£‡πâ‡∏≤‡∏á Loading Animation GUI
local loadingGui = Instance.new("ScreenGui")
loadingGui.Name = "LoadingAnimation"
loadingGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
loadingGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

-- ‡∏û‡∏∑‡πâ‡∏ô‡∏´‡∏•‡∏±‡∏á‡∏î‡∏≥‡πÇ‡∏õ‡∏£‡πà‡∏á‡πÉ‡∏™
local background = Instance.new("Frame")
background.Size = UDim2.new(1, 0, 1, 0)
background.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
background.BackgroundTransparency = 0.7
background.BorderSizePixel = 0
background.Parent = loadingGui

-- Container ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏≠‡∏ô‡∏¥‡πÄ‡∏°‡∏ä‡∏±‡∏ô
local container = Instance.new("Frame")
container.Size = UDim2.new(0, 200, 0, 200)
container.Position = UDim2.new(0.5, -100, 0.5, -100)
container.BackgroundTransparency = 1
container.Parent = loadingGui

-- ‡∏ß‡∏á‡∏Å‡∏•‡∏° Loading ‡∏Ç‡∏ô‡∏≤‡∏î‡πÉ‡∏´‡∏ç‡πà
local circle = Instance.new("Frame")
circle.Size = UDim2.new(0, 80, 0, 80)
circle.Position = UDim2.new(0.5, -40, 0.5, -40)
circle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
circle.BackgroundTransparency = 0.1
circle.Parent = container

local circleCorner = Instance.new("UICorner")
circleCorner.CornerRadius = UDim.new(1, 0)
circleCorner.Parent = circle

-- ‡∏ß‡∏á‡∏Å‡∏•‡∏°‡∏´‡∏°‡∏∏‡∏ô (‡∏ß‡∏á‡πÉ‡∏ô)
local spinner = Instance.new("Frame")
spinner.Size = UDim2.new(0, 60, 0, 60)
spinner.Position = UDim2.new(0.5, -30, 0.5, -30)
spinner.BackgroundColor3 = Color3.fromRGB(255, 200, 50)
spinner.BackgroundTransparency = 0
spinner.Parent = container

local spinnerCorner = Instance.new("UICorner")
spinnerCorner.CornerRadius = UDim.new(1, 0)
spinnerCorner.Parent = spinner

-- ‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏° Loading
local loadingText = Instance.new("TextLabel")
loadingText.Size = UDim2.new(1, 0, 0, 30)
loadingText.Position = UDim2.new(0, 0, 0.9, 0)
loadingText.BackgroundTransparency = 1
loadingText.TextColor3 = Color3.fromRGB(255, 255, 255)
loadingText.TextSize = 14
loadingText.Font = Enum.Font.GothamBold
loadingText.Text = "Loading..."
loadingText.Parent = container

-- ‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏ä‡∏∑‡πà‡∏≠‡∏™‡∏Ñ‡∏£‡∏¥‡∏õ‡∏ï‡πå‡∏ó‡∏µ‡πà‡∏´‡∏°‡∏∏‡∏ô
local rotatingText = Instance.new("TextLabel")
rotatingText.Size = UDim2.new(0, 120, 0, 30)
rotatingText.BackgroundTransparency = 1
rotatingText.TextColor3 = Color3.fromRGB(255, 200, 50)
rotatingText.TextSize = 12
rotatingText.Font = Enum.Font.GothamBold
rotatingText.Text = "Cmds by humanx1"
rotatingText.Parent = container

-- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô Animation
local runService = game:GetService("RunService")
local connection = nil

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏õ‡∏¥‡∏î Loading Screen
local function closeLoadingScreen()
    if connection then
        connection:Disconnect()
    end
    
    -- Animation ‡∏´‡∏≤‡∏¢‡πÑ‡∏õ‡∏≠‡∏¢‡πà‡∏≤‡∏á‡∏ä‡πâ‡∏≤‡πÜ
    for i = 1, 10 do
        container.BackgroundTransparency = i/10
        background.BackgroundTransparency = 0.7 + (i/10 * 0.3)
        container.Position = UDim2.new(0.5, -100, 0.5, -100 - (i * 5))
        task.wait(0.03)
    end
    
    loadingGui:Destroy()
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô Animation
local function startLoadingAnimation()
    local time = 0
    local radius = 70  -- ‡∏£‡∏±‡∏®‡∏°‡∏µ‡∏Å‡∏≤‡∏£‡∏´‡∏°‡∏∏‡∏ô
    
    connection = runService.RenderStepped:Connect(function(delta)
        time = time + delta * 2
        
        -- ‡∏´‡∏°‡∏∏‡∏ô‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏£‡∏≠‡∏ö‡πÜ ‡∏ß‡∏á‡∏Å‡∏•‡∏°
        local x = math.cos(time) * radius
        local y = math.sin(time) * radius
        rotatingText.Position = UDim2.new(0.5, x - 60, 0.5, y - 15)
        
        -- ‡∏´‡∏°‡∏∏‡∏ô‡∏ß‡∏á‡∏Å‡∏•‡∏°‡πÉ‡∏ô
        spinner.Rotation = time * 100
    end)
    
    -- ‡∏´‡∏•‡∏±‡∏á‡∏à‡∏≤‡∏Å 2 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ ‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ô‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡πÉ‡∏ô‡πÅ‡∏ä‡∏ó
    task.wait(1)
    
    -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏° Loading
    loadingText.Text = "‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏™‡πà‡∏á‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡πÉ‡∏ô‡πÅ‡∏ä‡∏ó..."
    
    -- ‡∏û‡∏¥‡∏°‡∏û‡πå‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡πÉ‡∏ô‡πÅ‡∏ä‡∏ó‡πÅ‡∏ö‡∏ö‡∏ó‡∏µ‡∏•‡∏∞‡∏ï‡∏±‡∏ß‡∏≠‡∏±‡∏Å‡∏©‡∏£
    local message = "SCRIPT CITY THAILAND 2 BY HUMANX1"
    
    -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏û‡∏¥‡∏°‡∏û‡πå‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡πÉ‡∏ô‡∏ä‡πà‡∏≠‡∏á‡πÅ‡∏ä‡∏ó
    local function typeInChat(text)
        -- ‡∏û‡∏¢‡∏≤‡∏¢‡∏≤‡∏°‡∏´‡∏≤‡∏ä‡πà‡∏≠‡∏á‡πÅ‡∏ä‡∏ó
        local chatBar = nil
        
        -- ‡∏´‡∏≤ ChatBar ‡∏à‡∏≤‡∏Å StarterGui
        for _, gui in pairs(game:GetService("StarterGui"):GetDescendants()) do
            if gui:IsA("TextBox") and (gui.Name == "ChatBar" or gui.Parent.Name == "ChatBar") then
                chatBar = gui
                break
            end
        end
        
        -- ‡∏´‡∏≤ ChatBar ‡∏à‡∏≤‡∏Å PlayerGui
        if not chatBar then
            for _, gui in pairs(game.Players.LocalPlayer.PlayerGui:GetDescendants()) do
                if gui:IsA("TextBox") and (gui.Name == "ChatBar" or gui.Parent.Name == "ChatBar") then
                    chatBar = gui
                    break
                end
            end
        end
        
        if chatBar then
            -- ‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡∏ä‡πà‡∏≠‡∏á‡πÅ‡∏ä‡∏ó‡∏°‡∏≠‡∏á‡πÄ‡∏´‡πá‡∏ô‡πÅ‡∏•‡∏∞‡πÇ‡∏ü‡∏Å‡∏±‡∏™
            chatBar.Visible = true
            chatBar:CaptureFocus()
            
            -- ‡∏û‡∏¥‡∏°‡∏û‡πå‡∏ó‡∏µ‡∏•‡∏∞‡∏ï‡∏±‡∏ß‡∏≠‡∏±‡∏Å‡∏©‡∏£
            local currentText = ""
            for i = 1, #text do
                currentText = string.sub(text, 1, i)
                chatBar.Text = currentText
                
                -- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÄ‡∏≠‡∏ü‡πÄ‡∏ü‡∏Å‡∏ï‡πå‡∏û‡∏¥‡∏°‡∏û‡πå
                rotatingText.Text = "Typing" .. string.rep(".", i % 4)
                
                task.wait(0.05)  -- ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÄ‡∏£‡πá‡∏ß‡πÉ‡∏ô‡∏Å‡∏≤‡∏£‡∏û‡∏¥‡∏°‡∏û‡πå
            end
            
            -- ‡∏Å‡∏î Enter ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏™‡πà‡∏á
            chatBar.Text = text
            task.wait(0.5)
            
            -- ‡∏û‡∏¢‡∏≤‡∏¢‡∏≤‡∏°‡∏™‡πà‡∏á‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°
            pcall(function()
                -- ‡∏ß‡∏¥‡∏ò‡∏µ‡∏ó‡∏µ‡πà 1: ‡πÉ‡∏ä‡πâ SayMessageRequest
                local chatEvents = game:GetService("ReplicatedStorage"):FindFirstChild("DefaultChatSystemChatEvents")
                if chatEvents then
                    local sayMessageRequest = chatEvents:FindFirstChild("SayMessageRequest")
                    if sayMessageRequest then
                        sayMessageRequest:FireServer(text, "All")
                    end
                end
            end)
            
            pcall(function()
                -- ‡∏ß‡∏¥‡∏ò‡∏µ‡∏ó‡∏µ‡πà 2: ‡πÉ‡∏ä‡πâ TextChatService
                local textChatService = game:GetService("TextChatService")
                if textChatService then
                    local channel = textChatService.TextChannels:FindFirstChild("RBXGeneral")
                    if channel then
                        channel:SendAsync(text)
                    end
                end
            end)
            
            -- ‡∏õ‡∏¥‡∏î‡∏ä‡πà‡∏≠‡∏á‡πÅ‡∏ä‡∏ó
            chatBar:ReleaseFocus()
            task.wait(0.2)
            chatBar.Visible = false
            
            return true
        end
        
        return false
    end
    
    -- ‡∏û‡∏¥‡∏°‡∏û‡πå‡πÉ‡∏ô‡πÅ‡∏ä‡∏ó
    local chatSuccess = typeInChat(message)
    
    -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏° Loading ‡∏ï‡∏≤‡∏°‡∏ú‡∏•‡∏•‡∏±‡∏û‡∏ò‡πå
    if chatSuccess then
        loadingText.Text = "‚úÖ ‡∏™‡πà‡∏á‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!"
        rotatingText.Text = "READY!"
        task.wait(1)
    else
        loadingText.Text = "‚ö†Ô∏è ‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡∏™‡πà‡∏á‡∏Ç‡πâ‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡πÉ‡∏ô‡πÅ‡∏ä‡∏ó‡πÑ‡∏î‡πâ"
        rotatingText.Text = "BUT SCRIPT READY!"
        task.wait(1.5)
    end
    
    -- ‡∏õ‡∏¥‡∏î Loading Screen
    closeLoadingScreen()
    
    return chatSuccess
end

-- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô Animation ‡πÅ‡∏•‡∏∞‡∏Å‡∏£‡∏∞‡∏ö‡∏ß‡∏ô‡∏Å‡∏≤‡∏£
task.spawn(startLoadingAnimation)

